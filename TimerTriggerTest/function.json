{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "*/10 * * * * *"
    },
    {
      "name": "outputEventHub",
      "type": "eventHub",
      "direction": "out",
      "connection": "EVENT_HUB_CONNECTION_STRING",
      "eventHubName": "EVENT_HUB_NAME",
      "cardinality": "many"
    }
  ]
}